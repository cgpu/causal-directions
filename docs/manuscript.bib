    journal = {Proceedings of the Twenty Sixth Conference on Uncertainty in Artificial Intelligence (UAI-10)},
    keywords = {DAG,VALIDITY,artificial intelligence,backdoor criterion,causal effects,covariate adjustment,effects},
    pages = {527--536},
    title = {{On the validity of covariate adjustment for estimating causal effects}},
    year = {2010},
}

